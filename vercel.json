{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; style-src 'self' https: 'unsafe-inline'; script-src  'unsafe-inline' https://medium.com/; connect-src https://medium.com/; frame-ancestors 'none'; upgrade-insecure-requests; base-uri 'self';"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=63072000; preload; includeSubDomains"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "interest-cohort=()"
                }
            ]
        }
    ]
}